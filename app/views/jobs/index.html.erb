<h1>Listing jobs</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Application url</th>
      <th>Title</th>
      <th>Location</th>
      <th>Found by</th>
      <th>Status</th>
      <th>Notes</th>
      <th>Contact</th>
      <th>Strengths</th>
      <th>Intrique</th>
      <th>Intrique reason</th>
      <th>Due date</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.company %></td>
        <td><%= job.application_url %></td>
        <td><%= job.title %></td>
        <td><%= job.location %></td>
        <td><%= job.found_by %></td>
        <td><%= job.status %></td>
        <td><%= job.notes %></td>
        <td><%= job.contact %></td>
        <td><%= job.strengths %></td>
        <td><%= job.intrique %></td>
        <td><%= job.intrique_reason %></td>
        <td><%= job.due_date %></td>
        <td><%= job.user %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
